{
	"name": "dataSilverTransformations",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "source_dimFlight_parquet",
						"type": "DatasetReference"
					},
					"name": "sourceDimFlight"
				},
				{
					"dataset": {
						"referenceName": "source_dimAirline_parquet",
						"type": "DatasetReference"
					},
					"name": "sourceDimAirline"
				},
				{
					"dataset": {
						"referenceName": "source_dimPassenger_parquet",
						"type": "DatasetReference"
					},
					"name": "sourceDimPassenger"
				},
				{
					"dataset": {
						"referenceName": "source_sql_parquet",
						"type": "DatasetReference"
					},
					"name": "sourceSQLIngetsion"
				},
				{
					"dataset": {
						"referenceName": "source_github_json_http",
						"type": "DatasetReference"
					},
					"name": "sourceGituhubIngestion"
				}
			],
			"sinks": [
				{
					"linkedService": {
						"referenceName": "AzureDataLakeStorage1",
						"type": "LinkedServiceReference"
					},
					"name": "sinkdimflightparquet"
				},
				{
					"linkedService": {
						"referenceName": "AzureDataLakeStorage1",
						"type": "LinkedServiceReference"
					},
					"name": "sinkdimAirline"
				},
				{
					"linkedService": {
						"referenceName": "AzureDataLakeStorage1",
						"type": "LinkedServiceReference"
					},
					"name": "sinkdimpassengersilver"
				},
				{
					"linkedService": {
						"referenceName": "AzureDataLakeStorage1",
						"type": "LinkedServiceReference"
					},
					"name": "sinkfactbookingsilver"
				},
				{
					"linkedService": {
						"referenceName": "AzureDataLakeStorage1",
						"type": "LinkedServiceReference"
					},
					"name": "sinkAirportSilverData"
				}
			],
			"transformations": [
				{
					"name": "derivedColumnCleanAndSplit"
				},
				{
					"name": "derivedColumnTypeConversion"
				},
				{
					"name": "derivedColumnGenderConversion"
				},
				{
					"name": "selectSilverData"
				},
				{
					"name": "derivedColumnsDuration"
				},
				{
					"name": "derivedColumnCountryUppercase"
				},
				{
					"name": "derivedColumnTypeConversions"
				},
				{
					"name": "derivedColumnCheckNull"
				},
				{
					"name": "selectSilverDataGithub"
				},
				{
					"name": "selectSqlSilverData"
				},
				{
					"name": "selectAirlineSilverData"
				},
				{
					"name": "derivedColumnDuration"
				},
				{
					"name": "selectFlightSilverData"
				},
				{
					"name": "alterRow1"
				},
				{
					"name": "alterRow2"
				},
				{
					"name": "alterRow3"
				},
				{
					"name": "alterRow4"
				},
				{
					"name": "alterRow5"
				},
				{
					"name": "derivedColumnTypeflightId"
				},
				{
					"name": "derivedColumnTypeConversionspk"
				},
				{
					"name": "derivedColumnTypePassengerId"
				},
				{
					"name": "derivedColumntypeAirportId"
				}
			],
			"scriptLines": [
				"source(output(",
				"          flight_id as string,",
				"          flight_number as string,",
				"          departure_time as string,",
				"          arrival_time as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> sourceDimFlight",
				"source(output(",
				"          airline_id as string,",
				"          airline_name as string,",
				"          country as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> sourceDimAirline",
				"source(output(",
				"          passenger_id as string,",
				"          full_name as string,",
				"          gender as string,",
				"          age as string,",
				"          country as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> sourceDimPassenger",
				"source(output(",
				"          booking_id as integer,",
				"          passenger_id as integer,",
				"          flight_id as integer,",
				"          airline_id as integer,",
				"          origin_airport_id as integer,",
				"          destination_airport_id as integer,",
				"          booking_date as date,",
				"          ticket_cost as decimal(10,2),",
				"          flight_duration_mins as integer,",
				"          checkin_status as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> sourceSQLIngetsion",
				"source(output(",
				"          airport_id as short,",
				"          airport_name as string,",
				"          city as string,",
				"          country as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     documentForm: 'documentPerLine') ~> sourceGituhubIngestion",
				"sourceDimPassenger derive(first_name = split(full_name, ' ')[1],",
				"          last_name = split(full_name, ' ')[2]) ~> derivedColumnCleanAndSplit",
				"derivedColumnCleanAndSplit derive(age = toInteger(age),",
				"          gender = regexReplace(gender, 'M', 'Male')) ~> derivedColumnTypeConversion",
				"derivedColumnTypeConversion derive(gender = regexReplace(gender,'F','Female')) ~> derivedColumnGenderConversion",
				"derivedColumnTypePassengerId select(mapColumn(",
				"          passenger_id,",
				"          gender,",
				"          age,",
				"          country,",
				"          first_name,",
				"          last_name",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> selectSilverData",
				"sourceDimFlight derive(arrival_minutes = (toInteger(split(arrival_time,':')[1])*60)+toInteger(split(arrival_time,':')[2]),",
				"          depature_minutes = (toInteger(split(departure_time,':')[1])*60)+toInteger(split(departure_time,':')[2])) ~> derivedColumnsDuration",
				"sourceDimAirline derive(country = upper(country)) ~> derivedColumnCountryUppercase",
				"sourceGituhubIngestion derive(country = upper(country)) ~> derivedColumnTypeConversions",
				"sourceSQLIngetsion derive(ticket_cost = iif(isNull(ticket_cost),toDecimal(0.0),toDecimal(ticket_cost))) ~> derivedColumnCheckNull",
				"derivedColumntypeAirportId select(mapColumn(",
				"          airport_id,",
				"          airport_name,",
				"          city,",
				"          country",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> selectSilverDataGithub",
				"derivedColumnCheckNull select(mapColumn(",
				"          booking_id,",
				"          passenger_id,",
				"          flight_id,",
				"          airline_id,",
				"          origin_airport_id,",
				"          destination_airport_id,",
				"          booking_date,",
				"          ticket_cost,",
				"          flight_duration_mins,",
				"          checkin_status",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> selectSqlSilverData",
				"derivedColumnTypeConversionspk select(mapColumn(",
				"          airline_id,",
				"          airline_name,",
				"          country",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> selectAirlineSilverData",
				"derivedColumnsDuration derive(duration = iif(arrival_minutes>depature_minutes,arrival_minutes-depature_minutes,(1440-depature_minutes)+arrival_minutes)) ~> derivedColumnDuration",
				"derivedColumnTypeflightId select(mapColumn(",
				"          flight_id,",
				"          flight_number,",
				"          departure_time,",
				"          arrival_time,",
				"          arrival_minutes,",
				"          depature_minutes,",
				"          duration",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> selectFlightSilverData",
				"selectFlightSilverData alterRow(upsertIf(1==1)) ~> alterRow1",
				"selectAirlineSilverData alterRow(upsertIf(1==1)) ~> alterRow2",
				"selectSilverData alterRow(upsertIf(1==1)) ~> alterRow3",
				"selectSqlSilverData alterRow(upsertIf(1==1)) ~> alterRow4",
				"selectSilverDataGithub alterRow(upsertIf(1==1)) ~> alterRow5",
				"derivedColumnDuration derive(flight_id = toInteger(flight_id)) ~> derivedColumnTypeflightId",
				"derivedColumnCountryUppercase derive(airline_id = toInteger(airline_id)) ~> derivedColumnTypeConversionspk",
				"derivedColumnGenderConversion derive(passenger_id = toInteger(passenger_id)) ~> derivedColumnTypePassengerId",
				"derivedColumnTypeConversions derive(airport_id = toInteger(airport_id)) ~> derivedColumntypeAirportId",
				"alterRow1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'delta',",
				"     fileSystem: 'silver',",
				"     folderPath: 'DimFlight',",
				"     mergeSchema: false,",
				"     autoCompact: false,",
				"     optimizedWrite: false,",
				"     vacuum: 0,",
				"     deletable: false,",
				"     insertable: false,",
				"     updateable: false,",
				"     upsertable: true,",
				"     keys:['flight_id'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sinkdimflightparquet",
				"alterRow2 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'delta',",
				"     fileSystem: 'silver',",
				"     folderPath: 'DimAirline',",
				"     mergeSchema: false,",
				"     autoCompact: false,",
				"     optimizedWrite: false,",
				"     vacuum: 0,",
				"     deletable: false,",
				"     insertable: false,",
				"     updateable: false,",
				"     upsertable: true,",
				"     keys:['airline_id'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sinkdimAirline",
				"alterRow3 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'delta',",
				"     fileSystem: 'silver',",
				"     folderPath: 'DimPassenger',",
				"     mergeSchema: false,",
				"     autoCompact: false,",
				"     optimizedWrite: false,",
				"     vacuum: 0,",
				"     deletable: false,",
				"     insertable: false,",
				"     updateable: false,",
				"     upsertable: true,",
				"     keys:['passenger_id'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sinkdimpassengersilver",
				"alterRow4 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'delta',",
				"     fileSystem: 'silver',",
				"     folderPath: 'FactBookings',",
				"     mergeSchema: false,",
				"     autoCompact: false,",
				"     optimizedWrite: false,",
				"     vacuum: 0,",
				"     deletable: false,",
				"     insertable: false,",
				"     updateable: false,",
				"     upsertable: true,",
				"     keys:['booking_id'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sinkfactbookingsilver",
				"alterRow5 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'delta',",
				"     fileSystem: 'silver',",
				"     folderPath: 'DimAirport',",
				"     mergeSchema: false,",
				"     autoCompact: false,",
				"     optimizedWrite: false,",
				"     vacuum: 0,",
				"     deletable: false,",
				"     insertable: false,",
				"     updateable: false,",
				"     upsertable: true,",
				"     keys:['airport_id'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sinkAirportSilverData"
			]
		}
	}
}